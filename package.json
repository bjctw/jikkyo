{
  "name": "jikkyo",
  "version": "2.0.0-alpha.1",
  "description": "jikkyo",
  "main": "build",
  "electron": {
    "categoryType": "public.app-category.entertainment",
    "copyright": "(c) 2017 rot1024",
    "icon": "resources/jikkyo",
    "name": "jikkyo"
  },
  "scripts": {
    "start": "run-s dev",
    "postinstall": "electron-rebuild",
    "test": "cross-env NODE_ENV=test nyc ava",
    "test-watch": "run-s test -- --watch",
    "lint": "run-p eslint stylelint",
    "eslint": "eslint app tasks",
    "stylelint": "stylelint app/**/*.css",
    "start-prod": "electron .",
    "start-dev": "cross-env BABEL_ENV=electron NODE_ENV=development electron -r babel-register app/main",
    "dev": "run-p dev-server start-dev",
    "dev-server": "webpack-dev-server",
    "build": "run-p build-main build-renderer",
    "build-main": "cross-env BABEL_ENV=electron-build webpack --env.platform=electron --env.prod --colors --progress",
    "build-renderer": "webpack --env.prod --colors --progress",
    "package": "node tasks/package",
    "package-all": "run-s package -- --all"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rot1024/jikkyo.git"
  },
  "author": "rot1024",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rot1024/jikkyo/issues"
  },
  "homepage": "https://rot1024.github.io/jikkyo",
  "engines": {
    "node": ">=6"
  },
  "browserslist": [
    "Electron 1.6"
  ],
  "ava": {
    "babel": "inherit",
    "files": [
      "app/**/*.test.js"
    ],
    "require": [
      "babel-register",
      "./app/test/setup"
    ],
    "source": [
      "app/**/*.js"
    ]
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.0.11",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "devtron": "^1.2.1",
    "electron": "^1.6.2",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-packager": "^8.6.0",
    "electron-rebuild": "^1.5.7",
    "enzyme": "^2.8.0",
    "eslint": "^3.19.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^9.12.0",
    "minimist": "^1.2.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.2.0",
    "postcss-cssnext": "^2.10.0",
    "postcss-loader": "^1.3.3",
    "postcss-smart-import": "^0.6.11",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-test-renderer": "^15.5.4",
    "sinon": "^2.1.0",
    "source-map-support": "^0.4.14",
    "spectron": "^3.6.1",
    "style-loader": "^0.16.1",
    "stylelint": "^7.10.1",
    "stylelint-config-css-modules": "^0.1.0",
    "stylelint-config-standard": "^16.0.0",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.5.6",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-electron-store": "^0.4.1"
  }
}
